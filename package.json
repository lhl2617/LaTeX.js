{
	"name": "latex.js",
	"displayName": "latex.js",
	"description": "Provides preview and compilation of LaTeX.js in VSCode.",
	"version": "0.0.1",
	"publisher": "lhl2617",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.openPreview",
		"onCommand:extension.openPreviewToTheSide",
		"onCommand:extension.compile",
		"onLanguage:tex",
		"onLanguage:latex",
		"onLanguage:latex-expl3",
		"onLanguage:doctex"
	],
	"main": "./dist/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.openPreview",
				"title": "LaTeX.js: Open Preview",
				"group": "Preview",
				"description": "Open LaTeX.js HTML preview of the current file."
			},
			{
				"command": "extension.openPreviewToTheSide",
				"title": "LaTeX.js: Open Preview to The Side",
				"group": "Preview",
				"description": "Open LaTeX.js HTML preview of the current file to the side."
			},
			{
				"command": "extension.compile",
				"title": "LaTeX.js: Compile",
				"group": "Compilation",
				"description": "Compile the current file"
			}
		]
	},
	"configuration": {
		"title": "LaTeX.js",
		"properties": {
			"latex.js.pathToLatexJs": {
				"type": "string",
				"default": "",
				"group": "General",
				"description": "Path to installed `latex.js` file. Default (empty string) assumes latex.js is in PATH variables.",
				"scope": "resource"
			},
			"latex.js.refreshPreviewLive": {
				"type": "boolean",
				"default": true,
				"group": "Preview",
				"description": "Refresh preview live.",
				"scope": "resource"
			},
			"latex.js.compileOnSave": {
				"type": "boolean",
				"default": false,
				"group": "Compilation",
				"description": "Compile file on save (The file needs to be compiled manually first).",
				"scope": "resource"
			},
			"latex.js.generatorOptions.documentClass": {
				"type": "string",
				"default": "",
				"group": "Options",
				"description": "The default document class if a document without preamble is parsed.",
				"scope": "resource"
			},
			"latex.js.generatorOptions.hyphenate": {
				"type": "boolean",
				"default": true,
				"group": "Options",
				"description": "Enable or disable automatic hyphenation.",
				"scope": "resource"
			},
			"latex.js.generatorOptions.styles": {
				"type": "boolean",
				"default": true,
				"group": "Options",
				"description": "Absolute path to additional CSS stylesheets",
				"scope": "resource"
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"devDependencies": {
		"@types/node": "^12.19.5",
		"@types/vscode": "^1.51.0",
		"@types/which": "^1.3.2",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^3.10.1",
		"eslint": "^7.13.0",
		"ts-loader": "^7.0.5",
		"typescript": "^4.0.5",
		"webpack": "^4.44.2",
		"webpack-cli": "^3.3.12"
	},
	"dependencies": {
		"which": "^2.0.2"
	}
}